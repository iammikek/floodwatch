APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
# REDIS_HOST=redis (Sail) or 127.0.0.1 (local)
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Flood Watch cache (Redis). Use flood-watch-array when Redis unavailable (e.g. tests)
FLOOD_WATCH_CACHE_STORE=flood-watch
# Cache TTL in minutes. Set to 0 to disable caching.
FLOOD_WATCH_CACHE_TTL_MINUTES=0

# LLM Token Limits - Reduce these to optimize token usage and cost
# Max floods returned to LLM (default: 12)
# FLOOD_WATCH_LLM_MAX_FLOODS=12
# Max road incidents returned to LLM (default: 12)
# FLOOD_WATCH_LLM_MAX_INCIDENTS=12
# Max river monitoring stations returned to LLM (default: 8)
# FLOOD_WATCH_LLM_MAX_RIVER_LEVELS=8
# Max characters in flood forecast text (default: 1200)
# FLOOD_WATCH_LLM_MAX_FORECAST_CHARS=1200
# Max characters in individual flood warning messages (default: 150)
# FLOOD_WATCH_LLM_MAX_FLOOD_MESSAGE_CHARS=150
# Max characters in correlation summary (default: 8000)
# FLOOD_WATCH_LLM_MAX_CORRELATION_CHARS=8000

# LLM context limit in tokens (default: 110000). Reduce if you hit "context length exceeded" errors.
# FLOOD_WATCH_LLM_MAX_CONTEXT_TOKENS=110000

# LLM Cost Tracking (Admin Dashboard)
# Token pricing for gpt-4o-mini: input $0.15/1M, output $0.60/1M
# FLOOD_WATCH_LLM_COST_INPUT_PER_M=0.15
# FLOOD_WATCH_LLM_COST_OUTPUT_PER_M=0.60
# Monthly budget in USD (alerts at 80%)
# FLOOD_WATCH_LLM_BUDGET_MONTHLY=10
# Admin dashboard: prepaid credit for "remaining budget" (e.g. 10 for $10)
# FLOOD_WATCH_LLM_BUDGET_INITIAL=10

# LLM Prompt Version (default: v1, stored in resources/prompts/{version}/)
# FLOOD_WATCH_PROMPT_VERSION=v1

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# OpenAI â€“ 3 keys for different use cases
# Project key for chat/completions (REQUIRED)
OPENAI_API_KEY=
# Org ID (org-xxx) for multi-org accounts (OPTIONAL)
OPENAI_ORGANIZATION=
# Admin key (sk-admin-...) for Usage API; create at platform.openai.com/settings/organization/admin-keys (OPTIONAL)
OPENAI_ORG_ADMIN_KEY=
# Alternative to OPENAI_ORG_ADMIN_KEY
# OPENAI_ORG_API_KEY=
# Model selection: gpt-4o-mini (fast, cheap) or gpt-4o (powerful, expensive)
# OPENAI_MODEL=gpt-4o-mini

# Laravel Concurrency Driver
# Use 'sync' for local/testing (sequential), 'process' for production (parallel)
# CONCURRENCY_DRIVER=sync

# Flood Watch - Somerset Levels
# Default coordinates (Langport, Somerset)
# FLOOD_WATCH_LAT=51.0358
# FLOOD_WATCH_LNG=-2.8318
# FLOOD_WATCH_RADIUS_KM=15

# Optional: Register at https://developer.data.nationalhighways.co.uk/ for road closure data
# NATIONAL_HIGHWAYS_API_KEY=
# API v2.0: GET /roads/v2.0/closures?closureType=planned|unplanned (DATEX II v3.4)
# NATIONAL_HIGHWAYS_URL=https://api.data.nationalhighways.co.uk/roads/v2.0
# NATIONAL_HIGHWAYS_CLOSURES_PATH=closures
# NATIONAL_HIGHWAYS_FETCH_UNPLANNED=true
# NATIONAL_HIGHWAYS_TIMEOUT=25

# Circuit Breaker (Resilience for External APIs)
# Enabled by default to prevent cascading failures
# FLOOD_WATCH_CIRCUIT_BREAKER_ENABLED=true
# Number of consecutive failures before opening circuit (default: 5)
# FLOOD_WATCH_CIRCUIT_FAILURE_THRESHOLD=5
# Cooldown period in seconds before retrying (default: 60)
# FLOOD_WATCH_CIRCUIT_COOLDOWN=60

# Environment Agency API
# ENVIRONMENT_AGENCY_URL=https://environment.data.gov.uk/flood-monitoring
# ENVIRONMENT_AGENCY_TIMEOUT=25

# Flood Guidance Statement (5-day forecast) - public API, no key required
# FLOOD_FORECAST_URL=https://api.ffc-environment-agency.fgs.metoffice.gov.uk

# Optional: Ko-fi, Buy Me a Coffee, etc. When set, shows a low-friction donation link in the footer.
# DONATION_URL=https://ko-fi.com/automicalabs

# Admin: users who register with this email receive admin role (Pulse access)
# ADMIN_EMAIL=mike@automica.io

# Observability (Telescope at /telescope, Pulse at /pulse)
# Set to false in production or restrict dashboard access
# TELESCOPE_ENABLED=true
# PULSE_ENABLED=true
# PULSE_PATH=pulse
# Pulse uses database storage by default (same connection as DB_CONNECTION)
