APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
# REDIS_HOST=redis (Sail) or 127.0.0.1 (local)
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Flood Watch cache (Redis). Use flood-watch-array when Redis unavailable (e.g. tests)
FLOOD_WATCH_CACHE_STORE=flood-watch
# Cache TTL in minutes. Set to 0 to disable caching.
FLOOD_WATCH_CACHE_TTL_MINUTES=0

# LLM context limit (default 100000). Reduce if you hit "context length exceeded" errors.
# FLOOD_WATCH_LLM_MAX_CONTEXT_TOKENS=100000

# Admin dashboard: prepaid credit for "remaining budget" (e.g. 10 for $10)
# FLOOD_WATCH_LLM_BUDGET_INITIAL=10

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# OpenAI â€“ 3 keys for different use cases
# Project key for chat/completions
OPENAI_API_KEY=
# Org ID (org-xxx) for multi-org accounts
OPENAI_ORGANIZATION=
# Admin key (sk-admin-...) for Usage API; create at platform.openai.com/settings/organization/admin-keys
OPENAI_ORG_ADMIN_KEY=
# Alternative to OPENAI_ORG_ADMIN_KEY
# OPENAI_ORG_API_KEY=
# OPENAI_MODEL=gpt-4o-mini

# Flood Watch - Somerset Levels
# Optional: Register at https://developer.data.nationalhighways.co.uk/ for road closure data
# NATIONAL_HIGHWAYS_API_KEY=
# API v2.0: GET /roads/v2.0/closures?closureType=planned|unplanned (DATEX II v3.4)
# NATIONAL_HIGHWAYS_URL=https://api.data.nationalhighways.co.uk/roads/v2.0
# NATIONAL_HIGHWAYS_CLOSURES_PATH=closures
# NATIONAL_HIGHWAYS_FETCH_UNPLANNED=true

# Flood Guidance Statement (5-day forecast) - public API, no key required
# FLOOD_FORECAST_URL=https://api.ffc-environment-agency.fgs.metoffice.gov.uk

# Optional: Ko-fi, Buy Me a Coffee, etc. When set, shows a low-friction donation link in the footer.
# DONATION_URL=https://ko-fi.com/automicalabs

# Admin: users who register with this email receive admin role (Pulse access)
# ADMIN_EMAIL=mike@automica.io

# Observability (Telescope at /telescope, Pulse at /pulse)
# Set to false in production or restrict dashboard access
# TELESCOPE_ENABLED=true
# PULSE_ENABLED=true
# PULSE_PATH=pulse
# Pulse uses database storage by default (same connection as DB_CONNECTION)
